    <form method="post">
        <input type="hidden" asp-for="eva_momentos_metodologias_estatus.IdEstatusDet" />
        <input type="submit" value="Borrar" class="btn btn-danger" />
        <a class="btn btn-info btn-md active" role="button" asp-page="./Index">Cancelar</a>
