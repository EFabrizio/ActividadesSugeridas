            @Html.DisplayFor(model => model.eva_momentos_metodologias_estatus.Actual)
        </dd>
        <dt>
           Observación
        </dt>
        <dd>
            @Html.DisplayFor(model => model.eva_momentos_metodologias_estatus.Observacion)
        </dd>
        <dt>
           Usuario registro
        </dt>
        <dd>
            @Html.DisplayFor(model => model.eva_momentos_metodologias_estatus.IdUsuarioReg)
