@page
@model ActividadesSugeridasRazorPages.Pages.Eva_cat_tipo_actividades_sugeridas.CreateModel

@{
    ViewData["Title"] = "Create";
}



<h2>Crear Nuevo Tipo de Actividad Sugerida</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Eva_cat_tipo_actividad_sugerida.DesTipoActividadSug" class="control-label">Descripción de Tipo Actividad</label>
                <input role="button" asp-for="Eva_cat_tipo_actividad_sugerida.DesTipoActividadSug" class="form-control" />
                <span asp-validation-for="Eva_cat_tipo_actividad_sugerida.DesTipoActividadSug" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input  class="btn btn-success btn-md active"  type="submit" value="Crear"  />
            </div>
        </form>
    </div>
</div>

<div>
    <a class="btn btn-info btn-md active" role="button" asp-page="Index">Cancelar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
