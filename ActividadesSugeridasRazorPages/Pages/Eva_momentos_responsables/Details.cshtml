@page
@model ActividadesSugeridasRazorPages.Pages.Eva_momentos_responsables.DetailsModel

@{
    ViewData["Title"] = "Details";
}



<div>
    <h2>Detalle de responsable</h2>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           Responsable
        </dt>
        <dd>
            @Html.DisplayFor(model => model.evas_momentos_responsables.cat_generales.DesGeneral)
        </dd>
        <dt>
            Tipo de Responsable
        </dt>
        <dd>
            @Html.DisplayFor(model => model.evas_momentos_responsables.cat_tipo_generales.DesTipo)
        </dd>
        <dt>
            Competencia
        </dt>
        <dd>
            @Html.DisplayFor(model => model.evas_momentos_responsables.eva_cat_competencias.DesCompetencia)
        </dd>
        <dt>
            Momento
        </dt>
        <dd>
            @Html.DisplayFor(model => model.evas_momentos_responsables.eva_momentos_metodologia.DesMomento)
        </dd>
        <dt>
           Persona
        </dt>
        <dd>
            @Html.DisplayFor(model => model.evas_momentos_responsables.rh_cat_personas.Nombre )
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-default btn-md active" role="button" asp-page="./Edit" asp-route-id="@Model.evas_momentos_responsables.IdResponsable">Editar</a> |
    <a class="btn btn-info btn-md active" role="button" asp-page="./Index">Regresar</a>
</div>
